    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@ng-bootstrap/ng-bootstrap": "^19.0.1",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
